# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = false
indent_style = tab
# Set default charset
charset = utf-8
trim_trailing_whitespace = true
indent_size = 4

# Custom Coding Styles for Java files
[*.yml]
indent_style = space
indent_size = 2

[pom.xml]
insert_final_newline = true
